{
  "name": "adfsusermappingapi",
  "version": "0.1.0",
  "description": "ADFS User mapping API that accepts a username, and returns the mapped association for UPN.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build && node ./dist/server.js",
    "devvscode": "npm run build && node --nolazy --inspect=5858 ./dist/server.js",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node ./dist/server.js",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmhardison/adfsusermappingapi.git"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Jonathan Hardison <jmh@jonathanhardison.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmhardison/adfsusermappingapi/issues"
  },
  "homepage": "https://github.com/jmhardison/adfsusermappingapi#readme",
  "dependencies": {
    "lodash": ">=4.17.11",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "concat-map": "0.0.1",
    "detect-indent": "^6.0.0",
    "eslint": "^5.16.0",
    "invariant": "^2.2.2",
    "jsesc": "^2.5.1",
    "json5": "^0.5.1",
    "to-fast-properties": "^2.0.0",
    "trim-right": "^1.0.1",
    "snyk": "^1.167.2"
  },
  "devDependencies": {
    "lodash": ">=4.17.11",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^10.0.1",
    "babel-helpers": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "mongoose": "^5.5.11",
    "nodemon": "^1.19.1"
  },
  "snyk": true
}
